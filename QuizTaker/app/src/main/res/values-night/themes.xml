<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.QuizMaster" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/teal_200</item>
    </style>
    <style name="CardViewStyle" parent="CardView">
        <item name="cardBackgroundColor">#0F0F0F</item>
    </style>

    <style name="CardViewStyle.Dark" parent="CardView">
        <item name="cardBackgroundColor">#BDBBBB</item>
    </style>
    <color name="bottomNavigationColor">#f5f5f5</color>
    <color name="bottomNavigationTextColor">#212121</color>
    <color name="bottomNavigationBackground">#212121</color>
</resources>